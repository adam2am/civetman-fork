{
	"name": "base",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "civetman build && vite build",
		"test": "civetman build && vitest run",
		"coverage": "civetman build && vitest run --coverage",
		"dev": "civetman dev & vitest dev",
		"postinstall": "civetman build"
	},
	"dependencies": {
		"civetman": "workspace:*",
		"type-fest": "^3.5.7",
		"typescript": "^4.9.5"
	},
	"devDependencies": {
		"@types/node": "^18.13.0",
		"unplugin-auto-import": "^0.14.3",
		"vite": "^4.1.1",
		"vite-plugin-dts": "2.0.0-beta.0",
		"vitest": "^0.28.5"
	},
	"files": [
		"./dist/**/*",
		"tsconfig.json"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts"
}
